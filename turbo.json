{
  "globalDependencies": ["**/.env.*"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "@kami-ui/next-theme": {
      "dependsOn": ["@kami-ui/react-theme-common#build"]
    },
    "@kami-ui/react-theme": {
      "dependsOn": ["@kami-ui/react-theme-common#build"]
    },
    "@kami-ui/storybook": {
      "dependsOn": ["@kami-ui/next-theme#build"]
    }
  }
}

{
  "name": "@kami-ui/theme-shop",
  "description": "A theme shop exporting multiple themes.",
  "homepage": "https://github.com/hymnsOfWeb/kami-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/hymnsOfWeb/kami-ui"
  },
  "license": "ISC",
  "version": "0.0.1-alpha.3",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "kami-ui",
    "kui",
    "material-ui",
    "mui",
    "kami",
    "react",
    "theme",
    "shop",
    "design language",
    "design system",
    "dls",
